{
  "name": "deploy-directory-on-branch-to-gh-pages",
  "version": "0.2.5",
  "description": "A Node library that makes deploying a directory on a branch to GitHub pages easy and automatic.",
  "bin": {
    "deploy-directory-on-branch-to-gh-pages": "bin/deploy-directory-on-branch-to-gh-pages.js"
  },
  "main": "index.js",
  "files": [
    "bin/",
    "lib/"
  ],
  "scripts": {
    "test": "npm-run-all check-release-to-github lint check-format jest",
    "lint": "eslint index.js lib",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "format": "prettier-eslint \"**/*.js\" --write",
    "check-format": "prettier-eslint \"**/*.js\" --list-different",
    "check-release-to-github": "release-to-github-with-changelog-pre-release-checks",
    "release-to-github": "release-to-github-with-changelog"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oliverviljamaa/deploy-directory-on-branch-to-gh-pages.git"
  },
  "keywords": [
    "deploy",
    "gh-pages",
    "github",
    "pages",
    "circleci",
    "branch"
  ],
  "author": "Oliver Viljamaa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oliverviljamaa/deploy-directory-on-branch-to-gh-pages/issues"
  },
  "homepage": "https://github.com/oliverviljamaa/deploy-directory-on-branch-to-gh-pages#readme",
  "dependencies": {
    "@octokit/rest": "^15.15.1",
    "commander": "^2.19.0",
    "gh-pages": "^2.0.1",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "prettier-eslint-cli": "^4.7.1",
    "release-to-github-with-changelog": "^1.2.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint \"**/*.js\" --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
